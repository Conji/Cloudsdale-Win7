<Page x:Name="BrowserPage" x:Class="Cloudsdale_Lib_Win7.Browser"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="324.382"
	Title="Browser" Width="392.226">

    <Grid Margin="0,0,0,284">
        <TextBox x:Name="WebAddress" HorizontalAlignment="Left" Height="23" Margin="78,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="265"/>
        <Button Content="&lt;" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="22" Height="23" Click="Button_Click_2"/>
        <Button Content="&gt;" HorizontalAlignment="Left" Margin="37,10,0,0" VerticalAlignment="Top" Width="26" Height="23" Click="Button_Click_3"/>
        <Button Content="Go" HorizontalAlignment="Left" Margin="348,10,0,0" VerticalAlignment="Top" Width="34" Height="23" Click="Button_Click_1" IsDefault="True"/>
        <WebBrowser x:Name="WebBrowser" Margin="0,38,0,-260" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto"/>
        <Button Content="Go back to Cloudsdale!" Margin="0,0,0,-285" Height="25" VerticalAlignment="Bottom" Click="button4_click"/>

    </Grid>
</Page>
